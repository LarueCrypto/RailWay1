{
  "name": "goal-quest",
  "version": "1.0.0",
  "description": "Transform your personal development journey into an epic RPG adventure with gamified habit tracking and goal management",
  "type": "module",
  "keywords": [
    "habit-tracker",
    "goal-management",
    "gamification",
    "productivity",
    "rpg",
    "solo-leveling",
    "personal-development",
    "achievement-system",
    "react",
    "typescript"
  ],
  "scripts": {
    "dev": "tsx watch --env-file=.env server/index.ts",
    "build": "tsx script/build.ts && vite build",
    "start": "NODE_ENV=production tsx server/index.ts",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit push && drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "railway:deploy": "railway up",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
